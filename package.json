{
  "name": "@bgoff1/game-launcher",
  "version": "1.0.0",
  "description": "Game Launcher made in Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^22.3.2"
  },
  "dependencies": {
    "electron-store": "^5.1.0"
  },
  "repository": "https://github.com/bgoff1/game-launcher.git",
  "author": "Judah Goff <bgoff1@harding.edu>",
  "license": "MIT",
  "build": {
    "appId": "bgoff1.game-launcher",
    "productName": "Game Launcher",
    "win": {
      "target": "msi",
      "icon": "icon.png",
      "artifactName": "game-launcher-${version}.${ext}"
    }
  }
}
